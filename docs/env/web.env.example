# API base URL
VITE_API_URL=http://localhost:4000

# Stripe publishable key (only if you enable real Stripe flows)
VITE_STRIPE_PUBLISHABLE_KEY=
 
# Feature flags
# Explicitly disable Stripe UI by default (only set to 'true' when you have keys)
VITE_ENABLE_STRIPE=false
# Enable/disable ecommerce module in the web app UI
# Set to 'true' to show ecommerce screens; leave unset/false to hide (Phase 1 defaults to hidden)
VITE_ENABLE_ECOMMERCE=true
